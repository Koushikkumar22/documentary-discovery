  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/api/documentaries/random",
      "destination": "/api/random"
    },
    {
      "source": "/api/documentaries",
      "destination": "/api/index"
    }
  ]
}