{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "functions": {
    "api/**/*.ts": {
      "runtime": "now-node@2.0.0",
      "memory": 1024
    }
  },
  "rewrites": [
    {
      "source": "/api/documentaries/random",
      "destination": "/api/documentaries"
    },
    {
      "source": "/api/documentaries",
      "destination": "/api/documentaries"
    }
  ]
}